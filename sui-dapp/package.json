{
  "name": "sui-dapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "resolutions": {
    "@mysten/sui": "1.43.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production tsc && vite build && npm run postbuild",
    "postbuild": "cp public/404.html dist/404.html 2>/dev/null || copy public\\404.html dist\\404.html || true",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx}\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:walrus": "npm run build:prod && site-builder publish --epochs 10 dist/",
    "deploy:walrus:30d": "npm run build:prod && site-builder publish --epochs 30 dist/"
  },
  "dependencies": {
    "@mysten/dapp-kit": "0.19.6",
    "@mysten/enoki": "^0.12.12",
    "@mysten/messaging": "^0.1.0",
    "@mysten/sui": "1.43.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.87.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.7.3",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.555.0",
    "next-themes": "^0.4.6",
    "poseidon-lite": "^0.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.0",
    "react-router-dom": "^7.9.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  }
}
